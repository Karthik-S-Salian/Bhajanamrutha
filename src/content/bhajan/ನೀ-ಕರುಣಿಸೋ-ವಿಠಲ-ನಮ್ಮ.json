    "english": [
        [
            "Nee karuniso vithala namma saako pandari vithala ||pa||"
        ],
        [
            "vithala vithala panduranga jai jai vithala panduranga ||"
        ],
        [
            "Doshigalolage hiriyanu naa nirdoshigalarasane vithala |",
            "saasiveyashtu bhakutiyanariyeno shesha shayana hari vithala ||1||"
        ],
        [
            "Bhavasaagaradolu muluguve summane avalokisuvare vithala |nava nava vishayake higgutaliha manadavasara kaayo vithala ||2||"
        ],
        [
            "Nishidina ashana vasanake henaguve husi sangrahiside vithala | nishedha maayaa illavo narakada gasanege anjuve vithala ||3||"
        ],
        [
            "Jaavake maaduve saavira tappanu kaayo dayambudhi vithala | shreevara prasanna venkata vithala jeevake hone nee vithala ||4||"
        ]
    ],
