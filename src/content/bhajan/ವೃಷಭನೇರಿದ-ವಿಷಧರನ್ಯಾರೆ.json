        [
            "\u0c95\u0cc8\u0cb2\u0cbe\u0cb8\u0c97\u0cbf\u0cb0\u0cbf\u0caf \u0ca6\u0cca\u0cb0\u0cc6\u0caf\u0cbf\u0cb5\u0ca8\u0cae\u0ccd\u0cae \u2013 \u0c85\u0ca6\u0cc1 \u0c85\u0cb2\u0ccd\u0cb2\u0ca6\u0cc6 \u0c95\u0cc7\u0cb3\u0cc6",
